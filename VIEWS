CREATE DATABASE COLL;
USE COLL;

CREATE TABLE STUDENT(
ROLLNO INT PRIMARY KEY,
NAME VARCHAR(20),
MARKS INT,
CITY VARCHAR(20)
);

INSERT INTO STUDENT (ROLLNO, NAME, MARKS, CITY) VALUES
(101, 'ANIL', 78, 'BANGLORE'),
(102, 'BHUMIKA', 93, 'MUMBAI'),
(103, 'CHETAN', 85, 'DELHI'),
(104, 'PRANAV', 99, 'DELHI'),
(105, 'AMOGH', 56, 'BANGLORE'),
(106, 'KISHAN', 92, 'DELHI');

CREATE VIEW VIEW1 AS
SELECT ROLLNO, MARKS FROM STUDENT; 

SELECT *
FROM VIEW1
WHERE MARKS>90;

SELECT * FROM VIEW1;
